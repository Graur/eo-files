+package org.eolang.fs
+rt jvm org.eolang:eo-files:0.0.0

[path] > file
  path > @

  # Is it directory?
  [] > is-dir /bool

  # File exists?
  [] > exists /bool

  # Removes the file and returns TRUE if success
  [] > rm /bool

  # Turn it into an "output" with a single attribute
  # "write", which accepts one attribute "data"
  [mode] > as-output /?
