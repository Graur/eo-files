+package org.eolang.fs
+alias org.eolang.fs.file
+alias org.eolang.fs.tmpdir
+junit

[] > check-if-it-is-directory
  is-dir. > @
    file
      "."

[] > check-if-file-exists
  not. > @
    exists.
      file
        "some-absent-file.txt"

[] > deletes-single-file-in-temp
  rm. > @
    tmpfile.
      tmpdir
